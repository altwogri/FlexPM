{
  "type": "object",
  "properties": {
    "ismytasks": {
      "type": "identifier",
      "selector": "#ctl00_MainContent_lblTskSection",
      "control": "exists"
    },
    "mytasks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "action": {
            "type": "action"
          },
          "task": {
            "type": "string",
            "selector": ".dxgvDataRow_PlasticBlue",
            "selectIndex": 1,
            "control": "property.innerText"
          },
          "topic": {
            "type": "string"
          },
          "subgroup": {
            "type": "string"
          },
          "state": {
            "type": "string"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "ismytasks"
      ],
      "screens": [
        "mytasks"
      ]
    }
  ]
}
