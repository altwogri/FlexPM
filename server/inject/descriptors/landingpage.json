{
  "type": "object",
  "properties": {
    "islanding": {
      "type": "identifier",
      "selector": ".main_container",
      "control": "exists"
    },
    "clicktasks": {
      "type": "action",
      "selector": "a",
      "control": "action.link",
      "selectContains": "My Tasks"
    }
  },
  "connect": [
    {
      "match": [
        "islanding"
      ],
      "action": "clicktasks",
      "model": "landingpage"
    }
  ]
}
